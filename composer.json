{
  "name": "benignware/wp-swiper-shortcode",
  "description": "Swiper integration for Wordpress",
  "version": "0.0.12",
  "type": "wordpress-plugin",
  "license": "MIT",
  "homepage": "http://github.com/benignware-labs/wp-swiper-shortcode",
  "authors": [
    {
      "name": "Rafael Nowrotek",
      "email": "mail@benignware.com",
      "homepage": "https://github.com/benignware"
    }
  ],
  "keywords": [
    "wordpress", "wordpress-plugin"
  ],
  "support": {
    "issues": "https://github.com/benignware-labs/wp-swiper-shortcode/issues"
  },
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org"
  }],
  "require": {
    "php": ">=5.3.0",
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "wpackagist-plugin/featured-galleries": ">=1.7.1"
  },
  "extra": {
    "webroot-dir": "wp-core",
  	"webroot-package": "wordpress",
    "installer-paths": {
      "wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "wp-content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  }
}
